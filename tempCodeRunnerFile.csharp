string tamaño;
Console.Write("Ingrese el tamaño del vector: ");
tamaño = Console.ReadLine();

valor = new int[Int.Parse(tamaño)];
    for (int i = 0; i < tamaño; i++)
    {
        string num;
        Console.Write("Ingrese un valor "+(i+1)+": ");
        num = Console.ReadLine();
        valor[i] = Int.Parse(num);
    }

    for (int i = 0; i < tamaño; i++)
    {
        Console.Write("["+valor[i]+"] ");
    }
